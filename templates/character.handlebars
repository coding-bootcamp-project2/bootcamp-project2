<!-- Character Template -->
<div id="character-details-template" style="display:none">
    <p><strong>ID:</strong> {{ID}}</p>
    <p><strong>User ID:</strong> {{user_id}}</p>
    <p><strong>Class ID:</strong> {{class_id}}</p>
    <p><strong>Race ID:</strong> {{race_id}}</p>
    <p><strong>Level:</strong> {{level}}</p>
    <p><strong>Skill:</strong> {{skill}}</p>
    <p><strong>Armor:</strong> {{armor}}</p>
    <p><strong>Hit Points:</strong> {{hit_points}}</p>
    <p><strong>Attributes:</strong></p>
    <ul>
        <li><strong>Strength:</strong> {{strength}}</li>
        <li><strong>Dexterity:</strong> {{dexterity}}</li>
        <li><strong>Constitution:</strong> {{constitution}}</li>
        <li><strong>Intelligence:</strong> {{intelligence}}</li>
        <li><strong>Wisdom:</strong> {{wisdom}}</li>
        <li><strong>Charisma:</strong> {{charisma}}</li>
    </ul>
    <p><strong>Date Created:</strong> {{date_created}}</p>
</div>
