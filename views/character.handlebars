<!-- Character Template -->
<div class="character-template">
  <div class="strong character-info">
    <h2 id ="character-name" class = "box-outline ">Character Name{{character_name}}</h2>
    <H2 id ="character-crn" class = "box-outline">Class/Race/Alignment</H2>
  </div>
  <div class="character-stats">
    <h2 class = "fb box-outline">Hit Points:</h2>
    <h2 class = "fb box-outline">Armor Class:</h2>
    <h2 class = "fb box-outline">Initiative:</h2>
  </div>
  <div class="character-image">
    <h2>Character Image</h2>
    <img>
  </div>
  <div class="character-abilities">
    <h2 class = "box-outline">Strength:</h2>
    <h2 class = "box-outline">Intelligence:</h2>
    <h2 class = "box-outline">Dexterity:</h2>
    <h2 class = "box-outline">Wisdom:</h2>
    <h2 class = "box-outline">Constitution:</h2>
    <h2 class = "box-outline">Charisma:</h2>
  </div>
